rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{uid} {
      allow read, write: if request.auth.uid == uid;
    }
    match /common/{document=**} {
      allow read;
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}